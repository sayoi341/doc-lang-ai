{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "hono",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "lib": [
            "esnext"
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx"
    }
}